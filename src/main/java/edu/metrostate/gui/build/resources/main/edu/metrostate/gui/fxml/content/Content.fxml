<?import java.net.URL?>
<BorderPane xmlns="http://javafx.com/javafx/8.0.65"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="edu.metrostate.gui.content.ContentController">
    <stylesheets>
        <URL value="@Content.css" />
    </stylesheets>
    <top>
        <HBox alignment="CENTER_RIGHT" prefHeight="64.0" minHeight="64.0" maxHeight="64.0" style="-fx-padding: 12">
            <Button fx:id="logoutButton" text="Logout" />
        </HBox>
    </top>
    <center>
        <TabPane fx:id="tabPane">
            <Tab fx:id="listTab" text="List Example" closable="false">
                <fx:include fx:id="studentListView" source="../studentlist/StudentList.fxml" />
            </Tab>
            <Tab fx:id="imageTab" text="Upload Image Example" closable="false">
                <fx:include source="../image/Image.fxml" />
            </Tab>
        </TabPane>
    </center>
</BorderPane>
